<Window x:Class="mya.NewUsluga"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:mya"
        mc:Ignorable="d"
        Title="NewUsluga" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBox  x:Name="Name" Grid.Column="1" Width="100px" Height="50px" Background="Black"  Foreground="Red"  ></TextBox>
        <Button x:Name="OkButton" Content="Ok" Margin="70 50" Grid.Column="1" Grid.Row="2" Click="OkButton_Click"/>
    </Grid>
</Window>
